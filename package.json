{
  "name": "mips-uwaterloo",
  "displayName": "UWaterloo MIPS Support",
  "description": "Provides syntax highlighting and snippets for MIPS assembly language used in the University of Waterloo's CS241 and CS230 courses.",
  "publisher": "qhyun-uwaterloo",
  "version": "1.2.0",
  "engines": {
    "vscode": "^1.52.0"
  },
  "icon": "assets/icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/qhyun2/mips-uwaterloo"
  },
  "categories": [
    "Programming Languages",
    "Snippets"
  ],
  "contributes": {
    "languages": [
      {
        "id": "mips",
        "aliases": [
          "UW MIPS Assembly",
          "mips"
        ],
        "extensions": [
          ".s",
          ".mips",
          ".asm"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "mips",
        "scopeName": "source.mips",
        "path": "./syntaxes/mips.tmLanguage.json"
      }
    ],
    "snippets": [
      {
        "language": "mips",
        "path": "./snippets/snippets.json"
      }
    ]
  }
}
